<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Debug">
        <Item>
          <IdeaActionID>ActivateDebugToolWindow</IdeaActionID>
          <ShortCut>'⌘2'</ShortCut>
          <Description>Debug</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Debug 'Development: wheel-api'">
        <Item>
          <IdeaActionID>Debug</IdeaActionID>
          <ShortCut>'⌃⌥D'</ShortCut>
          <Description>Debug 'Development: wheel-api'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Declaration">
        <Item>
          <IdeaActionID>GotoDeclaration</IdeaActionID>
          <ShortCut>'⌘B'</ShortCut>
          <Description>Declaration</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Preferences...">
        <Item>
          <IdeaActionID>ShowSettings</IdeaActionID>
          <ShortCut>'⌘,'</ShortCut>
          <Description>Preferences...</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Rename...">
        <Item>
          <IdeaActionID>RenameElement</IdeaActionID>
          <ShortCut>'⇧F6'</ShortCut>
          <Description>Rename...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Rerun 'Development: wheel-api'">
        <Item>
          <IdeaActionID>Rerun</IdeaActionID>
          <ShortCut>'⌘R'</ShortCut>
          <Description>Rerun 'Development: wheel-api'</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Resume Program">
        <Item>
          <IdeaActionID>Resume</IdeaActionID>
          <ShortCut>'⌥⌘R' or 'F9'</ShortCut>
          <Description>Resume Program</Description>
          <Count>6</Count>
        </Item>
      </Statistic>
      <Statistic Action="Show Local Changes as UML">
        <Item>
          <IdeaActionID>UML.ShowChanges</IdeaActionID>
          <ShortCut>'⌥⇧⌘D'</ShortCut>
          <Description>Show Local Changes as UML</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Step Over">
        <Item>
          <IdeaActionID>StepOver</IdeaActionID>
          <ShortCut>'F8'</ShortCut>
          <Description>Step Over</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
      <Statistic Action="Terminal">
        <Item>
          <IdeaActionID>ActivateTerminalToolWindow</IdeaActionID>
          <ShortCut>'⌘4'</ShortCut>
          <Description>Terminal</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Toggle Line Breakpoint">
        <Item>
          <IdeaActionID>ToggleLineBreakpoint</IdeaActionID>
          <ShortCut>'⌘F8'</ShortCut>
          <Description>Toggle Line Breakpoint</Description>
          <Count>7</Count>
        </Item>
      </Statistic>
      <Statistic Action="Update route list">
        <Item>
          <IdeaActionID>railways.UpdateRoutesList</IdeaActionID>
          <ShortCut>'⇧⌘\'</ShortCut>
          <Description>Update route list</Description>
          <Count>20</Count>
        </Item>
      </Statistic>
    </option>
    <option name="suppressed">
      <Statistic Action="Event Log">
        <Item>
          <IdeaActionID>ActivateEventLogToolWindow</IdeaActionID>
          <ShortCut>'⌘7'</ShortCut>
          <Description>Event Log</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Key Promoter X">
        <Item>
          <IdeaActionID>ActivateKeyPromoterXToolWindow</IdeaActionID>
          <Description>Key Promoter X</Description>
        </Item>
      </Statistic>
    </option>
  </component>
</application>